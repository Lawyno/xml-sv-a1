	<xs:complexType name="Name_Ergaenzung_v3_Ctp">
		<xs:annotation>
			<xs:documentation>Beschreibung der Geburtsangaben, Versicherungsnummer und Staatsangehoerigkeit</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geburtsname" type="btst:Geburtsname_Typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Geburtsname der Person</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Geburtsdatum" type="btst:Geburtsdatum_unbekannt_Typ">
				<xs:annotation>
					<xs:documentation>Geburtsdatum der Person in der Form: jjjj-mm-tt
Bei ausländischen Staatsbürgern kann der Tag oder Monat der Geburt unbekannt sein. In diesen Fällen ist der Tag oder Monat mit "00" anzugeben. Bei der Ausstellung einer Bescheinigung, wird das Geburtsdatum auf 01.01. verändert, damit eine Nutzung im Rahmen des europäischen Datenaustausches erfolgen kann</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Geburtsort" type="btst:Ort_Typ">
				<xs:annotation>
					<xs:documentation>Geburtsort der Person</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Geburtsland" type="btst:Laendercode_Typ">
				<xs:annotation>
					<xs:documentation>Geburtsland des Arbeitnehmers nach Anlage 8 des gemeinsamen Rundschreibens "Meldeverfahren zur Sozialversicherung". </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Versicherungsnummer" type="btst:VSNR_Typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Versicherungsnummer des Arbeitnehmers in der Form: bbttmmjjassp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Staatsangehoerigkeit" type="btst:Laendercode_Typ">
				<xs:annotation>
					<xs:documentation>Dabei handelt es sich um den Staatsangehoerigkeitsschluessel des Arbeitnehmers nach Anlage 8 des gemeinsamen Rundschreibens "Meldeverfahren zur Sozialversicherung".</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Schriftwechsel_Kontakt">
		<xs:annotation>
			<xs:documentation>Angaben zum Schriftwechsel bei Anträgen an die DVKA</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Schriftwechsel">
				<xs:annotation>
					<xs:documentation>Der Ansprechpartner kann wie folgt ausgeprägt sein:
1 = Schriftwechsel mit dem Arbeitgeber / Kontaktperson
2 = Schriftwechsel mit der bevollmaechtigten Stelle
</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="btst:Art_Kontaktadresse_Typ">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Bezeichnung_Bevollmaechtigte_Stelle" type="btst:Name_AG_Typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hier ist die aktuelle Firmenbezeichnung der bevollmächtigten Stelle anzugeben,</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Postalischer_Kontakt" type="ns1:Anschrift_Ctp" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Angaben zur Anschrift der Kontaktperson</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Aktenzeichen_Kontakt" type="btst:Aktenzeichen_Typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hier ist das Aktenzeichen der fuer den Schriftwechsel mit dem Ansprechpartner anzugeben.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Elektronischer_Kontakt" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Telefonnummer" type="btst:Rufnummer_Typ">
							<xs:annotation>
								<xs:documentation>Rufnummer des Ansprechpartners nach DIN 5008:
Die Telefonnummer ist funktionsbezogen durch je ein Leerzeichen zu gliedern, vor der Durchwahlnummer steht ein Bindestrich.
Beispiele:
Einzelanschluss
04404 912145
Durchwahlanschluss
04401 922-122
International
+49 4401 922-131
(Die laenderbezogene Zusatznummer sollte durch das Zeichen + vor der Landesvorwahl dargestellt werden (z.B. statt 0049 besser +49).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="E_Mail" type="btst:Email_Typ" minOccurs="0">
							<xs:annotation>
								<xs:documentation>E-Mail Adresse des Arbeitgebers.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Steuerungsdaten_106a_Ctp">
		<xs:annotation>
			<xs:documentation>Steuerungsdaten ohne Vorgangs-ID und Stornogrund</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Absendernummer" type="btst:Absendernummer_Typ">
				<xs:annotation>
					<xs:documentation>Dabei handelt es sich um die Absendernummer der absendenden Stelle. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Empfaengernummer" type="btst:Absendernummer_Typ">
				<xs:annotation>
					<xs:documentation>In diesem Feld ist die Absendernummer der zustaendigen Stelle einzutragen. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Datum_Erstellung" type="btst:Datum_Zeit_Typ">
				<xs:annotation>
					<xs:documentation>Angabe des Erstelldatums der Meldung.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Datensatz_ID">
				<xs:annotation>
					<xs:documentation>Datensatz_ID analog der Ausgestaltung der uebrigen Meldeverfahren der Arbeitgeber. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="btst:DatensatzID_Typ"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Aktenzeichen_Verursacher" type="btst:Aktenzeichen_Verursacher_Typ">
				<xs:annotation>
					<xs:documentation>Das Aktenzeichen Versursacher steht dem Arbeitgeber zur freien Verfügung.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stornierung" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Stornokennzeichen" type="btst:Stornokennzeichen_Typ">
							<xs:annotation>
								<xs:documentation>Kennzeichen zur Stornierung der Meldung. Fuer eine Stornierung ist das Kennzeichen "J"zu uebermitteln. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Datensatz_ID_Ursprungsmeldung" type="btst:DatensatzID_Typ"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Steuerungsdaten_AG_v2_Ctp">
		<xs:annotation>
			<xs:documentation>Steuerungsdaten für die DVKA mit Vorgangs-ID und Stornogrund</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Absendernummer" type="btst:Absendernummer_Typ">
				<xs:annotation>
					<xs:documentation>Dabei handelt es sich um die Absendernummer der absendenden Stelle. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Empfaengernummer" type="btst:Absendernummer_Typ">
				<xs:annotation>
					<xs:documentation>In diesem Feld ist die Absendernummer der zustaendigen Stelle einzutragen. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Datum_Erstellung" type="btst:Datum_Zeit_Typ">
				<xs:annotation>
					<xs:documentation>Angabe des Erstelldatums der Meldung.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Prod_ID" type="btst:Prod_ID_Typ">
				<xs:annotation>
					<xs:documentation>Hier ist die Prod-ID des Entgeltabrechnungsprogramms oder der Ausfuellhilfe einzutragen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Mod_ID" type="btst:Mod_ID_Typ">
				<xs:annotation>
					<xs:documentation>Hier ist die Mod-ID des Entgeltabrechnungsprogramms oder der Ausfuellhilfe einzutragen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Datensatz_ID">
				<xs:annotation>
					<xs:documentation>Datensatz_ID analog der Ausgestaltung der uebrigen Meldeverfahren der Arbeitgeber. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="btst:DatensatzID_Typ"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Vorgangs_ID" type="btst:DatensatzID_Typ">
				<xs:annotation>
					<xs:documentation>Die Vorgangs-ID ist ein für die meldende Stelle eindeutiges Zuord-nungskriterium für den Meldevorgang, welches unverändert auch für mögliche Stornierungen und Neumeldungen innerhalb des Meldevorgangs verwendet wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Aktenzeichen_Verursacher" type="btst:Aktenzeichen_Verursacher_Typ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Das Aktenzeichen Versursacher steht dem Arbeitgeber zur freien Verfügung.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stornierung" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Stornokennzeichen" type="btst:Stornokennzeichen_Typ">
							<xs:annotation>
								<xs:documentation>Kennzeichen zur Stornierung der Meldung. Fuer eine Stornierung ist das Kennzeichen "J"zu uebermitteln. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Stornogrund">
							<xs:annotation>
								<xs:documentation>1 = Zeitraum verkürzt sich: beschäftigte Person arbeitet nicht mehr im Aus-land
2 = Zeitraum verkürzt sich: beschäftigte Person ist weiter in dem/n betref-fenden Mitgliedstaat/en erwerbstätig
3 = Zeitraum verkürzt sich: Person ist nicht mehr bei diesem Arbeitgeber beschäftigt
4 = Beschäftigte Person ist in mehreren Mitgliedstaaten tätig. Der Ab-schluss einer Ausnahmevereinbarung ist nicht länger erforderlich.
5 = Person erfüllt die Entsendevoraussetzungen, so dass der gesendete Antrag nicht erforderlich ist.
6 = Die übermittelten Angaben waren fehlerhaft. Neuer Antrag mit korrekten Angaben folgt.
7 = Die beschäftigte Person hat ihren Lebensmittelpunkt nicht länger in Deutschland
8 = Aufnahme zusätzlicher Erwerbstätigkeit/en: ursprünglicher Sachverhalt liegt nicht mehr vor
9 = Die beschäftigte Person ist nicht länger in mehreren Mitgliedstaaten tätig</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:totalDigits value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Datensatz_ID_Ursprungsmeldung" type="btst:DatensatzID_Typ"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
