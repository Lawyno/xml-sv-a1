	<xs:simpleType name="VSNR_Typ">
		<xs:annotation>
			<xs:documentation>
				Die Rentenversicherungsnummer (RVNR)  ist ein aus Buchstaben und Ziffern bestehendes Kennzeichen zur Identifikation von versicherten Personen in der Gesetzlichen Rentenversicherung der Bundesrepublik Deutschland. Die RVNR ist alphanumerisch und 12 stellig.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="tns:ISO88591_Typ">
			<xs:length value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Wirtschaftssektoren_Typ">
		<xs:annotation>
			<xs:documentation>
				Aufstellung der Wirtschaftssektoren
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="2"/>
			<xs:pattern value="(2[0-2]|1[0-9]|[1-9])"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>