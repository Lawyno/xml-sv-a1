<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- bearbeitet mit XMLSpy v2023 rel. 2 (x64) (http://www.altova.com) von Björn Scharatta (GKV-Spitzenverband) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.gkv-datenaustausch.de/XMLSchema/106-Basis/1.0" xmlns:svb="SV-Basis://XMLSchema/1.0" targetNamespace="http://www.gkv-datenaustausch.de/XMLSchema/106-Basis/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="SV-Basis://XMLSchema/1.0" schemaLocation="SV-Basis-1.0.0.xsd"/>
	<!--simpleTypes-->
	<xs:simpleType name="Ablehnungsgrund_Stp">
		<xs:annotation>
			<xs:documentation>Typ zur Übermittlung des Ablehnungsgrundes einer A1 Bescheinigung</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Adresszusatz_Stp">
		<xs:annotation>
			<xs:documentation>
				Definition des Geburtsnames
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Adresszusatz_Stp">
			<xs:maxLength value="40"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Aktenzeichen_Verursacher_Stp">
		<xs:restriction base="svb:Aktenzeichen_Stp">
			<xs:maxLength value="20"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Aktenzeichen_Schriftwechsel_Kontakt_Stp">
		<xs:restriction base="svb:Aktenzeichen_Stp">
			<xs:maxLength value="32"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Anforderungsgrund_Stp">
		<xs:annotation>
			<xs:documentation>
				RVBEA Anforderungsgrund
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Art_Kontaktadresse_Stp">
		<xs:annotation>
			<xs:documentation>
				Aufzaehlung der möglichen Kontaktstellen für den Schriftwechsel.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Bezeichnung_Rechtsform_Stp">
		<xs:annotation>
			<xs:documentation>
				Name der Firma
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dateifolgenummer_Typ">
		<xs:annotation>
			<xs:documentation>
				Beschreibung der Dateifolgenummer
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Email_Stp">
		<xs:annotation>
			<xs:documentation>Email-Adresse entsprechend RFC 3696; vgl. "3. Restrictions on email addresses"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="70"/>
			<xs:pattern value="[A-Za-z0-9äöüÄÖÜ!&quot;&apos;#\\$%&amp;\(\)\*\+\?\^`&lt;=&gt;.:;,/_-]+(@|§)[A-Za-z0-9äöüÄÖÜ!&quot;&apos;#\\$%&amp;\(\)\*\+\?\^`&lt;=&gt;:;,/_-]+([.]{1}[A-Za-z0-9äöüÄÖÜ!&quot;&apos;#\\$%&amp;\(\)\*\+\?\^`&lt;=&gt;:;,/_-]+)+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Familienname_Stp">
		<xs:annotation>
			<xs:documentation>
				Dieser Datentyp modelliert den Nachnamen einer Person.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Nachname_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Fehlernummer_Typ">
		<xs:annotation>
			<xs:documentation>
				Fehlernummer der Kernpruefung
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp"/>
	</xs:simpleType>
	<xs:simpleType name="Fehlertext_Typ">
		<xs:annotation>
			<xs:documentation>
				Fehlertext der Kernpruefung
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp"/>
	</xs:simpleType>
	<xs:simpleType name="Geburtsname_Stp">
		<xs:annotation>
			<xs:documentation>
				Definition des Geburtsnames
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Nachname_Stp">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Geldleistung_Stp">
		<xs:annotation>
			<xs:documentation>
				Einstelliges Kennzeichen mit numerischen Inhalt
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Grund_Abloesung_Stp">
		<xs:annotation>
			<xs:documentation>
				Gruende der Abloesung eines Arbeitnehmers im Ausland. 1 = Erkrankung der zuvor entsandten Person. 2 = Kuendigung der zuvor entsandten Person. 3 = betriebsbedingter Personalwechsel
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Grundrentennummer_Japan_Stp">
		<xs:annotation>
			<xs:documentation>japanische Grundrentennummer  </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Iata_Stp">
		<xs:annotation>
			<xs:documentation>Typ zur Übermittlung des 3 stelligen IATA Codes für Flughäfen.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Identitaetsnummer_Israel_Stp">
		<xs:annotation>
			<xs:documentation>israelische Identitätsnummer   </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Imo_Nummer_Stp">
		<xs:annotation>
			<xs:documentation>Datentyp zur Darstellung der eineindeutigen Schiffskennung.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="10"/>
			<xs:pattern value="IMO[0-9]{7}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Iso88591_Stp">
		<xs:annotation>
			<xs:documentation>Eine 'ISO88591_Typ' Zeichenkette enthält nur Zeichen aus dem Zeichensatz ISO-8859-1 ohne Zeilenumbrüche (CR/LF) und Tab. Dieser Datentyp sollte als Basis für einfache Zeichenkettenfelder verwendet werden, bei denen Zeilenumbrüche/Tabs in den Daten nicht erwünscht sind.
			</xs:documentation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:pattern value="[&#x0020;-&#x007e;&#x00a0;-&#x00ff;]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Iso88591_Text_Stp">
		<xs:annotation>
			<xs:documentation>
Eine 'ISO88591_Text_Stp' Zeichenkette enthält  Zeichen aus dem Zeichensatz ISO-8859-1.
Dieser Datentyp kann Zeilenumbrüche/Tabs enthalten und sollte als Basis für mehrzeilige Texte verwendet werden.
			</xs:documentation>
			<xs:appinfo/>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:pattern value="[\n\r\t&#x0020;-&#x00FF;]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Jahr_Stp">
		<xs:annotation>
			<xs:documentation>
				Numerischer Wert 1-9999, entspricht allen Inhalten einer maximal 4-stelligen Zahl außer 0
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Alphanumerisch_J_Stp">
		<xs:annotation>
			<xs:documentation>
				Einstelliges Kennzeichen mit alphanumerischen Inhalt "J".
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="J"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Alphanumerisch_N_Stp">
		<xs:annotation>
			<xs:documentation>
				Einstelliges Kennzeichen mit alphanumerischen Inhalt "N".
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Alphanumerisch_Stp">
		<xs:annotation>
			<xs:documentation>
				Einstelliges Kennzeichen mit alphanumerischen Inhalt "J" oder "N"
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="J"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Mwx_Stp">
		<xs:annotation>
			<xs:documentation>
				Alphanumerischer Inhalt mit entweder M(ännlich), W(eiblich), X(unbestimmt) oder D (divers)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="W"/>
			<xs:enumeration value="X"/>
			<xs:enumeration value="D"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Numerisch_1_2_3_4_Stp">
		<xs:restriction base="Kennzeichen_Numerisch_Stp">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Numerisch_1_2_3_Stp">
		<xs:restriction base="Kennzeichen_Numerisch_Stp">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Numerisch_1_2_Stp">
		<xs:restriction base="Kennzeichen_Numerisch_Stp">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Kennzeichen_Numerisch_Stp">
		<xs:annotation>
			<xs:documentation>
				Einstelliges Kennzeichen mit numerischen Inhalt
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Langtext_Stp">
		<xs:annotation>
			<xs:documentation>
				Fehlernummer der Kernpruefung
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Text_Stp">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Mitgliedsnummer_Stp">
		<xs:annotation>
			<xs:documentation>
				Beschreibung der Mitgliedsnummer der ABV.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="5"/>
			<xs:maxLength value="17"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Name_Ag_Stp">
		<xs:annotation>
			<xs:documentation>
				Name mit maximal 50 Stellen.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Name_Stp">
		<xs:annotation>
			<xs:documentation>
				Name mit maximal 30 Stellen.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Iso88591_Stp">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Namenstitel_Stp">
		<xs:annotation>
			<xs:documentation>
				Dieser Datentyp modelliert den Namenstitel einer Person.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Namenstitel_Stp">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Namenszusatz_Stp">
		<xs:annotation>
			<xs:documentation>
				Dieser Typ modelliert den Namenszusatz einer Person. Die erlaubten Werte sind durch Anlage 7 DEUEV vorgegeben.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Namenszusatz_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Ort_Stp">
		<xs:annotation>
			<xs:documentation>
				Das Element Ort enthält den Namen eines Ortes (Gemeinde, Ortschaft oder Stadt).
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Ort_Stp">
			<xs:maxLength value="34"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Pdf_Stp">
		<xs:annotation>
			<xs:documentation>
				Element fuer die Ubermittlung des PDF, welches als base64 kodiertes PDF/A uebertragen wird.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:base64Binary">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Postleitzahl_Stp">
		<xs:annotation>
			<xs:documentation>
				Eine Postleitzahl ist eine Angabe, um postalische Zustellgebiete unabhaengig von Gebietskoerperschaften (Gemeinde, Kreis, _) zu bezeichnen.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Postleitzahl_Stp">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Rechtsform_Stp">
		<xs:annotation>
			<xs:documentation>
				Rechtsform des Arbeitgebers. 1 = Personen oder Kapitalgesellschaft; 2 = Öffentlicher Arbeitgeber; 3 = Sonstiges
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="1"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Regelungen_Arbeitsvertrag_Stp">
		<xs:annotation>
			<xs:documentation>
				Aufzaehlung der möglichen Ausgestaltungen zum Arbeitsvertrag waehrend der Entsendung.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Response_Id_Stp">
		<xs:annotation>
			<xs:documentation>
				Response_ID des jeweiligen Kommunikationsservers.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="23"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Sozialversicherungsnummer_Usa_Stp">
		<xs:annotation>
			<xs:documentation>XXX-YY-ZZZZ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="11"/>
			<xs:maxLength value="11"/>
			<xs:pattern value="([0-9]{3}-[0-9]{2}-[0-9]{4})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Stornokennzeichen_Stp">
		<xs:annotation>
			<xs:documentation>
				Kennzeichen zur Stornierung des urspruenglichen Datensatzes.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="J"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Strasse_Stp">
		<xs:annotation>
			<xs:documentation>
				Definition des Geburtsnames
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Strasse_Stp">
			<xs:maxLength value="33"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Taetigkeitsschluessel_A1_Stp">
		<xs:annotation>
			<xs:documentation>
				Taetigkeitsschlüssel der Bundesagentur für Arbeit von Stelle 1 - 5
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Telearbeit_Stp">
		<xs:annotation>
			<xs:documentation>Datentyp zur Übermittlung zu welchem Anteil die Person die Beschäftigung in Telearbeit ausübt (1 - 100 %).
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="100"/>
			<xs:totalDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Versicherungsnummer_Chile_Stp">
		<xs:annotation>
			<xs:documentation>chilenische Versicherungsnummer </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Vorname_Stp">
		<xs:annotation>
			<xs:documentation>
				Dieser Datentyp modelliert den Vornamen einer Person.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Vorname_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Vorsatzwort_Stp">
		<xs:annotation>
			<xs:documentation>
				Dieser Datentyp modelliert den Namensvorsatz einer Person. Die erlaubten Werte sind durch Anlage 6 DEUEV vorgegeben.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="svb:Namensvorsatz_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Wirtschaftssektoren_Stp">
		<xs:annotation>
			<xs:documentation>
				Aufstellung der Wirtschaftssektoren
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="2"/>
			<xs:pattern value="(2[0-2]|1[0-9]|[1-9])"/>
		</xs:restriction>
	</xs:simpleType>
	<!--complexTypes-->
	<xs:complexType name="Vorherige_Taetigkeit_Ctp">
		<xs:sequence>
			<xs:element name="Vorherige_Taetigkeit_Ausland" type="Kennzeichen_Alphanumerisch_Stp" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="Angaben_Vorherige_Taetigkeit" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Wenn die betroffene Person in den letzten x Jahren im Entsendungsstart gearbeitet hat, dann müssen die Angaben zu Zeitraum, Arbeitgeber und Beschäftigungsort gemacht werden</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Taetigkeitsbereich_Umsatzanteil_Ctp">
		<xs:sequence>
			<xs:element name="Taetigkeitsbereich" type="Kennzeichen_Alphanumerisch_Stp" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Die Tätigkeit der betreffenden Person im Beschäftigungsstaat entspricht dem Tätigkeitsbereich unseres Unternehmens in Deutschland.
J = Ja
N = Nein
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Umsatzanteil" type="Kennzeichen_Alphanumerisch_Stp" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unser Unternehmen übt gemessen am Umsatz mindestens 25% seiner Geschäftstätigkeit in Deutschland aus?
J = Ja
N = Nein</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Angaben_Name_Ctp">
		<xs:annotation>
			<xs:documentation>Beschreibung der Namensbestandteile</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geschlecht" type="Kennzeichen_Mwx_Stp">
				<xs:annotation>
					<xs:documentation>Alphanumerischer Inhalt mit entweder M(ännlich), W(eiblich), X(unbestimmt) oder D(divers)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Vorname" type="Vorname_Stp">
				<xs:annotation>
					<xs:documentation>Vorname der Person </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Familienname" type="Familienname_Stp">
				<xs:annotation>
					<xs:documentation>Familienname der Person</xs:documentation>
				</xs:annotation>
			</xs:element>
