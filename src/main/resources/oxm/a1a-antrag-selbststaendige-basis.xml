<?xml version="1.0" encoding="utf-8" ?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
              package-name="de.lawyno.xml.sv.a1.basis" version="2.5">

    <xml-schema element-form-default="QUALIFIED" attribute-form-default="UNQUALIFIED"
                namespace="http://www.gkv-datenaustausch.de/Grundsaetze_Technik/XML/Basistypen_Complex_106a/1.0.0">
        <xml-ns prefix="ase" namespace-uri="http://www.gkv-datenaustausch.de/XMLSchema/A1_Selbststaendige/2.0" />
        <xml-ns prefix="btct"
                namespace-uri="http://www.gkv-datenaustausch.de/Grundsaetze_Technik/XML/Basistypen_Complex_106a/1.0.0" />
    </xml-schema>

    <java-types>
        <java-type name="SteuerungsdatenRueckmeldungCtp" xml-transient="true" />

        <java-type name="SteuerungsdatenAgV3Ctp">
            <xml-type name="Steuerungsdaten_106a_Ctp" />
<!--
            <xml-type name="Steuerungsdaten_106a_Ctp"
                      prop-order="absendernummer empfaengernummer datumErstellung datensatzId aktenzeichenVerursacher stornierung" />
-->
            <java-attributes>
                <xml-element java-attribute="datensatzId" name="Datensatz_ID" />
                <xml-transient java-attribute="produktIdentifier" />
                <xml-transient java-attribute="modifikationsIdentifier" />
                <xml-transient java-attribute="vorgangsId" />
            </java-attributes>
        </java-type>
        <java-type name="SteuerungsdatenAgV3Ctp$Stornierung">
            <java-attributes>
                <xml-element java-attribute="datensatzIdUrsprungsmeldung" name="Datensatz_ID_Ursprungsmeldung" />
            </java-attributes>
        </java-type>

        <java-type name="AngabenZurPersonMitglCtp">
            <xml-type namespace="http://www.gkv-datenaustausch.de/XMLSchema/A1_Selbststaendige/2.0" />
        </java-type>
        <java-type name="GrundangabeNameNamensergaenzungenCtp">
            <xml-type namespace="http://www.gkv-datenaustausch.de/XMLSchema/A1_Selbststaendige/2.0" />
            <java-attributes>
                <xml-element java-attribute="grundangabenName" name="Grundangabe_Name" />
                <xml-element java-attribute="namensergaenzungen" name="Namens_Ergaenzungen" />
            </java-attributes>
        </java-type>
        <java-type name="AnschriftPersonImAbkommensstaat">
            <java-attributes>
                <xml-element java-attribute="land" name="Laenderschluessel" />
            </java-attributes>
        </java-type>

        <java-type name="Schiff">
            <xml-type namespace="http://www.gkv-datenaustausch.de/XMLSchema/A1_Selbststaendige/2.0" />
            <java-attributes>
                <xml-element java-attribute="imoNummer" name="IMO_Nummer" />
            </java-attributes>
        </java-type>
    </java-types>

</xml-bindings>